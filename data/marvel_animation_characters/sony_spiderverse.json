  "role_definitions": {
    "protagonist": "Central character driving the main narrative",
    "deuteragonist": "Second most important character, often close ally to protagonist",
    "hero": "Character who performs heroic acts but isn't the main focus",
    "villain": "Primary antagonist with malevolent goals",
    "antagonist": "Character opposing the protagonist (not necessarily evil)",
    "henchman": "Subordinate serving a villain",
    "ally": "Character who assists the protagonist",
    "sidekick": "Constant companion to a main character",
    "love_interest": "Romantic focus for a main character",
    "comic_relief": "Character primarily providing humor",
    "supporting": "Important recurring character in secondary role",
    "minor": "Brief appearance with limited impact on plot"
  },
  "notes": {
    "animation_style": "Distinctive hybrid combining CGI with hand-drawn comic book aesthetics, halftone dots, and multiple art styles per universe",
    "production": "Sony Pictures Animation, not Disney/Marvel Animation",
    "beyond_cast_note": "Beyond the Spider-Verse cast is confirmed/expected based on December 2025 reporting; additional characters likely",
    "spider_society_note": "Across the Spider-Verse features 280+ Spider-variants; only named/speaking roles with identified actors included"
  }
}
